{
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font",
					"fontFace": []
				},
				{
					"fontFamily": "Inknut Antiqua",
					"slug": "inknut-antiqua",
					"fontFace": [
						{
							"fontFamily": "Inknut Antiqua",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/inknut-antiqua_600.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Noto Sans",
					"slug": "noto-sans",
					"fontFace": [
						{
							"fontFamily": "Noto Sans",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/noto-sans_200.ttf"
							]
						},
						{
							"fontFamily": "Noto Sans",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/noto-sans_600.ttf"
							]
						},
						{
							"fontFamily": "Noto Sans",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/noto-sans_regular.ttf"
							]
						}
					]
				}
			]
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2
}